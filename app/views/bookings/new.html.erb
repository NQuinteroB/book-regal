<h1>Create your booking</h1>
<%= simple_form_for [@book, @booking] do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :start_time, input_html: { autocomplete: "Starting Date" } %>
  <%= f.input :end_time, input_html: { autocomplete: "Ending Date" } %>
  </div>

  <div class="form-actions">
    <%= f.button :submit, "Create" %>
  </div>
<% end %>
