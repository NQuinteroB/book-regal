<h1>Create your booking</h1>
<%= simple_form_for [@book, @booking] do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :start_time, :order => [:month, :day], input_html: { autocomplete: "Starting Date" } %>
  <%= f.input :end_time, :order => [:month, :day], input_html: { autocomplete: "Ending Date" } %>
  </div>

  <div class="form-actions">
   <button class="btn btn-primary"><%= f.button :submit, "Create" %></button>
   <br>
    <%= link_to "Home", books_path %>
  </div>
<% end %>
